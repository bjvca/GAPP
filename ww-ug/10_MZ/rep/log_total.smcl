{txt}263{com}.                 replace  spg = 0             if h==0;
{txt}264{com}.                 gen h_flex`pass'  =h;
{txt}265{com}.                 gen pg_flex`pass' =pg;
{txt}266{com}.                 gen spg_flex`pass'=spg;
