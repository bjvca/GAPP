>         work/product_flex.inc
>         work/price_unit_flex.inc
>         work/quan_flex.inc
> */
> 
> 
> 
> ***************************************************************************;
. * Export price, codes and quantity data to text
> ***************************************************************************;
. use "$path/work/price_unit_flex_it$it_n.dta";
{txt}
{com}. gen reg=spdomain ;
{txt}
{com}. gen crap=1;
{txt}
{com}.  lab def crap
>          1  ".";
{txt}
