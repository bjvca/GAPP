.                             save `codmiss', replace;
{txt} 36{com}. *browse;
.                                   use "$path/work/price_unit_flex_it$it_n.dta", clear;
{txt} 37{com}.                         sort product;
{txt} 38{com}. *browse;
.                         merge product using `codmiss';
