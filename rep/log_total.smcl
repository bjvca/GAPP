.                 preserve;
{txt}120{com}.                                                 * Temporally adjusted expenditure ;
.                         use `contpi', clear;
{txt}121{com}.                                 keep hhid hhweight spdomain reg_tpi survquar tpi_trim cons_pc_tpi ;
