. gen crap=1;

.  lab def crap
>          1  ".";

.  lab val crap crap;

. outsheet product crap reg bswt price_uw$it_n using "$path\work\price_unit_flex.inc",  non
> ame replace ;

. collapse (min) spdomain, by(product);

